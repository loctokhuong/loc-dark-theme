{
  "name": "Simple dark theme",
  "colors": {
    "editor.background": "#0F111A",
    "editor.foreground": "#5f626c",
    "activityBarBadge.background": "#2f313a",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.background": "#2f313a",
    "statusBar.background": "#2f313a",
    "editorUnnecessaryCode.border": "#2f313a",
  },
  "tokenColors": [
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#cccfda"
      }
    },
    {
      "scope": ["keyword", "meta", "meta.function"],
      "settings": {
        "foreground": "#c789d6"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#878994"
      }
    },
    {
      "scope": ["support.class", "entity.name"],
      "settings": {
        "foreground": "#878994"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#878994"
      }
    },

    {
      "scope": ["meta", "source"],
      "settings": {
        "foreground": "#878994"
      }
    },

    {
      "scope": [
        "comment.line.double-slash",
        "punctuation.definition.comment",
        "comment.block"
      ],
      "settings": {
        "foreground": "#4d4f59"
      }
    }
  ]
}
